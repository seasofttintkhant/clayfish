.mida001{
  position: relative;
  color: white;
  background: #81d0cb;
  font-size:18px;
  line-height: 1.4;
  padding: 0.5em 0.5em 0.5em 1.8em;
}

mida001:before {/*疑似要素*/
  font-family: "Font Awesome 5 Free";
  content: "\f14a";
  position: absolute;
  left : 0.5em; /*左端からのアイコンまでの距離*/
}


.button01{
     display:block;
     width: 200px;
     height:50px;
     line-height: 50px;
	 color:#FFF;
     text-decoration: none;
     text-align: center;
     background-color: #f39800; /*ボタン色*/
     border-radius: 5px; /*角丸*/
     -webkit-transition: all 0.5s;
     transition: all 0.5s;
	 font-size:14px;
	 font-weight:bold;
}
.button01:hover{
     background-color: #f9c500; /*ボタン色*/
}


