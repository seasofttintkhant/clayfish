/* ========================================
	リンク
======================================== */
ul.slick-box3{
    list-style:none;
    width:100%;
	height:100%;
	padding:0px;
	margin:0px;
}

ul.slick-box3 li{
	padding: 0px;
	margin: 0px 0px 20px 20px;
	box-shadow: 10px 10px 15px -10px;
}

.slick-current {
  transform: scale(1);
  transform-origin: 50% 50%;
}
.slick-current + .slick-box3 {
  transform-origin: 0 50%;
  margin:0px 0px 0px 30px;
}