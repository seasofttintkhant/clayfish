/* ----- 「ページトップへ」の位置とリンク色 ----- */
p.page-up {
	text-align: right;
	margin: 5px 0px 0px 0px;		/* マージン 上・右・下・左 */
}
p.page-up a:link {
	color: #398930;		/* リンク・フォントの色 */
	text-decoration: none;	/* リンク線 */
}
p.page-up a:visited{
	color: #398930;		/* 訪問済みリンク・フォントの色 */
	text-decoration: none;	/* リンク線 */
}
p.page-up a:hover{
	color: #FFCC00;		/* ロールオーバー時・フォントの色 */
	text-decoration: none;	/* リンク線 */
}
p.page-up a:active {
	color: #FFCC00;		/* クリック時・フォントの色 */
	text-decoration: none;	/* リンク線 */
}


