.billdiscount01 th  { width: 30%; text-align: left; color:#2980ac; background-color:#edeeef; padding:10px 0px 10px 10px; font-weight:bold; }
.billdiscount01 td  { padding:10px 0px 10px 10px; }
 
@media only screen and (max-width:480px){
    .billdiscount01 { margin: 0px; }
    .billdiscount01 th{
    width: 100%;
    display: block;
    border-top: none;
	color:#2980ac;
    }
	
    .billdiscount01 td{
    width: 100%;
    display: block;
    border-top: none;
    }
	
  .billdiscount01 tr:first-child th { border-top: 1px solid #ddd; }
}

.company02 th  { width: 20%; text-align: left; }
 
@media only screen and (max-width:480px){
    .company02 { margin: 0px; }
    .company02 th,
    .company02 td{
    width: 100%;
    display: block;
    border-top: none;
    }
  .company02 tr:first-child th { border-top: 1px solid #ddd; }
}

.contact01 th  { width: 30%; text-align: left; }
 
